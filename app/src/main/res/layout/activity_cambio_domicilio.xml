<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="match_parent">





    <!--Primeros datos-->
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="1000dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Cambio de Domicilio"
                android:layout_centerHorizontal="true"
                android:textSize="25dp"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Ciudad:"
                android:textSize="20dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="85dp"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginTop="85dp"
                android:textSize="20dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="75dp"
                android:id="@+id/ciudad"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Localidad:"
                android:textSize="20dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="115dp"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginTop="115dp"
                android:textSize="20dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="100dp"
                android:id="@+id/localidad"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Colonia:"
                android:textSize="20dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="145dp"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginTop="145dp"
                android:textSize="20dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="75dp"
                android:id="@+id/colonia"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Calle:"
                android:textSize="20dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="175dp"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginTop="175dp"
                android:textSize="20dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="60dp"
                android:id="@+id/Calledom"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Número:"
                android:textSize="20dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="205dp"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginTop="205dp"
                android:textSize="20dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="75dp"
                android:id="@+id/numero"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Número Interior:"
                android:textSize="20dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="235dp"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginTop="235dp"
                android:textSize="20dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="150dp"
                android:id="@+id/numeroi"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Teléfono:"
                android:textSize="20dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="265dp"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginTop="265dp"
                android:textSize="20dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="90dp"
                android:id="@+id/telefono"
                />


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="350dp"
                android:orientation="horizontal"
                android:layout_alignTop="@id/telefono"
                android:layout_marginTop="50dp"
                android:id="@+id/map"
                >

                <RelativeLayout
                    android:id="@+id/uno"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@drawable/contorno"

                    >
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/dos"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@drawable/contorno"
                    android:layout_below="@id/uno"
                    android:layout_marginTop="50dp"
                    >
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@drawable/contorno"
                    android:layout_below="@id/dos"
                    android:layout_marginTop="50dp"
                    >
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="110dp"
                    android:layout_marginEnd="110dp"
                    android:id="@+id/center"
                    >

                    <RelativeLayout
                        android:id="@+id/cuatro"
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:background="@drawable/contorno"
                        android:layout_centerHorizontal="true"
                        >
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/cinco"
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:background="@drawable/contorno"
                        android:layout_below="@id/cuatro"
                        android:layout_marginTop="50dp"
                        android:layout_centerHorizontal="true">
                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:src="@drawable/casa"
                            android:layout_centerHorizontal="true"
                            android:id="@+id/casanorte"
                            android:visibility="invisible"
                            />
                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:src="@drawable/casa"
                            android:layout_centerHorizontal="true"
                            android:layout_alignParentBottom="true"
                            android:id="@+id/casasur"
                            android:visibility="invisible"
                            />
                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:src="@drawable/casa"
                            android:layout_centerVertical="true"
                            android:id="@+id/casaoeste"
                            android:visibility="invisible"
                            />
                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:src="@drawable/casa"
                            android:layout_centerVertical="true"
                            android:layout_alignParentEnd="true"
                            android:id="@+id/casaeste"
                            android:visibility="invisible"
                            />

                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:background="@drawable/contorno"
                        android:layout_below="@id/cinco"
                        android:layout_marginTop="50dp"
                        android:layout_centerHorizontal="true"
                        >
                    </RelativeLayout>
                    <TextView
                        android:id="@+id/callenorte"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Calle Norte"
                        android:layout_alignParentTop="true"
                        android:layout_marginTop="100dp"
                        android:layout_centerHorizontal="true"

                        />
                    <TextView
                        android:id="@+id/callesur"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Calle Sur"
                        android:layout_alignParentTop="true"
                        android:layout_marginTop="220dp"
                        android:layout_centerHorizontal="true"
                        />

                </RelativeLayout>
                <TextView
                    android:id="@+id/calleeste"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:text="Calle Este"
                    android:rotation="90"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginEnd="60dp"
                    />
                <TextView
                    android:id="@+id/calleoeste"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:text="Calle Oeste"
                    android:layout_centerVertical="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginStart="40dp"
                    android:rotation="90"
                    />
                <RelativeLayout
                    android:id="@+id/siete"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@drawable/contorno"
                    android:layout_alignParentEnd="true"
                    >
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/ocho"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@drawable/contorno"
                    android:layout_alignParentEnd="true"
                    android:layout_below="@id/siete"
                    android:layout_marginTop="50dp"
                    >
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@drawable/contorno"
                    android:layout_alignParentEnd="true"
                    android:layout_below="@id/ocho"
                    android:layout_marginTop="50dp"
                    >
                    <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:src="@drawable/flecha"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        />
                </RelativeLayout>








            </RelativeLayout>
            <Button
                android:id="@+id/aceptar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Aceptar"
                android:layout_marginTop="700dp"
                />
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>